#include <stdio.h>

// Função para calcular o MDC
int mdc(int a, int b) {
    while (b != 0) {
        int resto = a % b;
        a = b;
        b = resto;
    }
    return a;
}

int main() {
    int x, y;

    printf("Digite dois numeros: ");
    scanf("%d %d", &x, &y);

    printf("O MDC de %d e %d eh: %d\n", x, y, mdc(x, y));

    return 0;
}
