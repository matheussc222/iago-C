#include <stdio.h>

int main() {
    float n1, n2, n3, n4, media;

    printf("Digite as quatro notas (entre 0 e 10):\n");
    scanf("%f %f %f %f", &n1, &n2, &n3, &n4);

    // calcula a m√©dia
    media = (n1 + n2 + n3 + n4) / 4;

    printf("A media das notas eh: %.2f\n", media);

    return 0;
}
