#include <stdio.h>

int main() {
    int numeros[6] = {12, 45, 7, 89, 23, 4}; 
    int i;
    int maior = numeros[0];
    int menor = numeros[0];

    for (i = 1; i < 6; i++) {
        if (numeros[i] > maior) {
            maior = numeros[i];
        }
        if (numeros[i] < menor) {
            menor = numeros[i];
        }
    }
    
    printf("Maior valor: %d\n", maior);
    printf("Menor valor: %d\n", menor);
    
    return 0;
}
